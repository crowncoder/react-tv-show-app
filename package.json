{
  "name": "react-tv-show-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.20.0",
    "lodash-es": "^4.17.15",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:analyze": "react-app-rewired build --analyze",
    "test": "react-app-rewired test",
    "test:ci": "react-app-rewired test --watchAll=false",
    "test:coverage": "react-app-rewired test --coverage --watchAll=false",
    "eject": "react-app-rewired eject",
    "lint:no-fix": "eslint --cache src/**/*.js",
    "lint": "eslint --cache --fix src/**/*.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.18.2",
    "babel-plugin-import": "^1.13.1",
    "customize-cra": "^1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "husky": "^4.3.0",
    "react-app-rewired": "^2.1.6",
    "redux-mock-store": "^1.5.4",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint:no-fix && yarn test:ci"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
